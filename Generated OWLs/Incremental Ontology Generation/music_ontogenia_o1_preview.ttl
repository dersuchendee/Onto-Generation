@prefix : <http://example.org/music#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

######################
# Classes
######################

:Event a owl:Class ;
    rdfs:label "Event" ;
    rdfs:comment "An event represents something that happens at a certain time and place." .

:Performance a owl:Class ;
    rdfs:label "Performance" ;
    rdfs:comment "A live performance event featuring one or more agents performing at a certain time and place." ;
    rdfs:subClassOf :Event ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :performedBy ;
        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Agent
    ] , [
        a owl:Restriction ;
        owl:onProperty :takesPlaceAt ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Place
    ] , [
        a owl:Restriction ;
        owl:onProperty :hasTimeInterval ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :TimeInterval
    ] .

:Agent a owl:Class ;
    rdfs:label "Agent" ;
    rdfs:comment "An entity that can perform actions, such as a person or a band." ;
    owl:equivalentClass [
        a owl:Class ;
        owl:unionOf ( :Person :Band )
    ] .

:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "An individual human being." ;
    rdfs:subClassOf :Agent .

:Band a owl:Class ;
    rdfs:label "Band" ;
    rdfs:comment "A group of musicians." ;
    rdfs:subClassOf :Agent .

:TimeInterval a owl:Class ;
    rdfs:label "Time Interval" ;
    rdfs:comment "A period of time with a start date and an end date." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :startDate ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty :endDate ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

:Place a owl:Class ;
    rdfs:label "Place" ;
    rdfs:comment "A physical location or venue where events occur." .

######################
# Object Properties
######################

:performedBy a owl:ObjectProperty ;
    rdfs:label "performed by" ;
    rdfs:comment "Links a Performance to the Agent(s) who performed it." ;
    rdfs:domain :Performance ;
    rdfs:range :Agent ;
    owl:inverseOf :performs .

:performs a owl:ObjectProperty ;
    rdfs:label "performs" ;
    rdfs:comment "Indicates the Performance(s) that an Agent has performed." ;
    rdfs:domain :Agent ;
    rdfs:range :Performance ;
    owl:inverseOf :performedBy .

:takesPlaceAt a owl:ObjectProperty ;
    rdfs:label "takes place at" ;
    rdfs:comment "Links an Event to the Place where it occurred." ;
    rdfs:domain :Event ;
    rdfs:range :Place ;
    owl:inverseOf :locationOfEvent .

:locationOfEvent a owl:ObjectProperty ;
    rdfs:label "location of event" ;
    rdfs:comment "Indicates the Event(s) that took place at a Place." ;
    rdfs:domain :Place ;
    rdfs:range :Event ;
    owl:inverseOf :takesPlaceAt .

:hasTimeInterval a owl:ObjectProperty ;
    rdfs:label "has time interval" ;
    rdfs:comment "Links an Event to the Time Interval during which it took place." ;
    rdfs:domain :Event ;
    rdfs:range :TimeInterval .

######################
# Data Properties
######################

:startDate a owl:DatatypeProperty ;
    rdfs:label "start date" ;
    rdfs:comment "The start date of a Time Interval." ;
    rdfs:domain :TimeInterval ;
    rdfs:range xsd:date .

:endDate a owl:DatatypeProperty ;
    rdfs:label "end date" ;
    rdfs:comment "The end date of a Time Interval." ;
    rdfs:domain :TimeInterval ;
    rdfs:range xsd:date .

:agentName a owl:DatatypeProperty ;
    rdfs:label "agent name" ;
    rdfs:comment "The name of an Agent." ;
    rdfs:domain :Agent ;
    rdfs:range xsd:string .

:placeName a owl:DatatypeProperty ;
    rdfs:label "place name" ;
    rdfs:comment "The name of a Place." ;
    rdfs:domain :Place ;
    rdfs:range xsd:string .

