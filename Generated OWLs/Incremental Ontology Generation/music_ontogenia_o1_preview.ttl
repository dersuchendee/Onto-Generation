@prefix : <http://example.org/music#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

######################
# Classes
######################

:Album a owl:Class ;
    rdfs:label "Album" ;
    rdfs:comment "An Album is a collection of Tracks released together under a single title." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasTrack ;
        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Track
    ] .

:Track a owl:Class ;
    rdfs:label "Track" ;
    rdfs:comment "A Track is an individual piece of music on an Album." .

######################
# Object Properties
######################

:hasTrack a owl:ObjectProperty ;
    rdfs:label "has track" ;
    rdfs:comment "Relates an Album to its Tracks." ;
    rdfs:domain :Album ;
    rdfs:range :Track ;
    owl:inverseOf :isTrackOf .

:isTrackOf a owl:ObjectProperty ;
    rdfs:label "is track of" ;
    rdfs:comment "Relates a Track to the Album it is part of." ;
    rdfs:domain :Track ;
    rdfs:range :Album ;
    owl:inverseOf :hasTrack .

######################
# Data Properties
######################

:albumTitle a owl:DatatypeProperty ;
    rdfs:label "album title" ;
    rdfs:comment "The title of the Album." ;
    rdfs:domain :Album ;
    rdfs:range xsd:string .

:releaseDate a owl:DatatypeProperty ;
    rdfs:label "release date" ;
    rdfs:comment "The date the Album was released." ;
    rdfs:domain :Album ;
    rdfs:range xsd:date .

:trackTitle a owl:DatatypeProperty ;
    rdfs:label "track title" ;
    rdfs:comment "The title of the Track." ;
    rdfs:domain :Track ;
    rdfs:range xsd:string .

:trackNumber a owl:DatatypeProperty ;
    rdfs:label "track number" ;
    rdfs:comment "The number of the Track on the Album." ;
    rdfs:domain :Track ;
    rdfs:range xsd:integer .
