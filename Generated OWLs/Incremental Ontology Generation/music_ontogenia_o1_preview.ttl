
@prefix : <http://example.org/music#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

######################
# Classes
######################

:Person a owl:Class ;
    rdfs:label "Person" .

:Instrument a owl:Class ;
    rdfs:label "Instrument" .

:MusicalInstrument a owl:Class ;
    rdfs:subClassOf :Instrument ;
    rdfs:label "Musical Instrument" .

:Band a owl:Class ;
    rdfs:label "Band" .

:Role a owl:Class ;
    rdfs:label "Role" .

:Agent a owl:Class ;
    rdfs:label "Agent" .

######################
# Object Properties
######################

:playsInstrument a owl:ObjectProperty ;
    rdfs:label "plays instrument" ;
    rdfs:domain :Person ;
    rdfs:range :MusicalInstrument .

:hasMember a owl:ObjectProperty ;
    rdfs:label "has member" ;
    rdfs:domain :Band ;
    rdfs:range :Person .

:isMemberOf a owl:ObjectProperty ;
    rdfs:label "is member of" ;
    owl:inverseOf :hasMember ;
    rdfs:domain :Person ;
    rdfs:range :Band .

:hasRole a owl:ObjectProperty ;
    rdfs:label "has role" ;
    rdfs:domain :Person ;
    rdfs:range :Role .

:playsRole a owl:ObjectProperty ;
    rdfs:label "plays role" ;
    owl:inverseOf :hasRole ;
    rdfs:domain :Role ;
    rdfs:range :Person .

######################
# Data Properties
######################

:personName a owl:DatatypeProperty ;
    rdfs:label "person name" ;
    rdfs:domain :Person ;
    rdfs:range xsd:string .

:instrumentName a owl:DatatypeProperty ;
    rdfs:label "instrument name" ;
    rdfs:domain :Instrument ;
    rdfs:range xsd:string .

:bandName a owl:DatatypeProperty ;
    rdfs:label "band name" ;
    rdfs:domain :Band ;
    rdfs:range xsd:string .

######################
# Individuals
######################

:AnthonyKiedis a :Person ;
    :personName "Anthony Kiedis" ;
    :playsInstrument :Vocals ;
    :hasRole :Vocalist ;
    :isMemberOf :RedHotChiliPeppers .

:Flea a :Person ;
    :personName "Flea" ;
    :playsInstrument :BassGuitar, :Trumpet, :Keyboards, :Vocals ;
    :hasRole :Bassist, :Trumpeter, :Keyboardist, :Vocalist ;
    :isMemberOf :RedHotChiliPeppers .

:JohnFrusciante a :Person ;
    :personName "John Frusciante" ;
    :playsInstrument :Guitar ;
    :hasRole :Guitarist ;
    :isMemberOf :RedHotChiliPeppers, :TheMarsVolta .

:ChadSmith a :Person ;
    :personName "Chad Smith" ;
    :playsInstrument :Drums ;
    :hasRole :Drummer ;
    :isMemberOf :RedHotChiliPeppers .

:RedHotChiliPeppers a :Band ;
    :bandName "Red Hot Chili Peppers" ;
    :hasMember :AnthonyKiedis, :Flea, :JohnFrusciante, :ChadSmith .

:Vocals a :MusicalInstrument ;
    :instrumentName "Vocals" .

:BassGuitar a :MusicalInstrument ;
    :instrumentName "Bass Guitar" .

:Trumpet a :MusicalInstrument ;
    :instrumentName "Trumpet" .

:Keyboards a :MusicalInstrument ;
    :instrumentName "Keyboards" .

:Guitar a :MusicalInstrument ;
    :instrumentName "Guitar" .

:Drums a :MusicalInstrument ;
    :instrumentName "Drums" .

######################
# Roles
######################

:Vocalist a :Role ;
    rdfs:label "Vocalist" .

:Bassist a :Role ;
    rdfs:label "Bassist" .

:Trumpeter a :Role ;
    rdfs:label "Trumpeter" .

:Keyboardist a :Role ;
    rdfs:label "Keyboardist" .

:Guitarist a :Role ;
    rdfs:label "Guitarist" .

:Drummer a :Role ;
    rdfs:label "Drummer" .

######################
# Restrictions
######################

:Person rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :playsInstrument ;
    owl:someValuesFrom :MusicalInstrument
] .


@prefix : <http://example.org/music#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

######################
# Classes
######################

:Person a owl:Class ;
    rdfs:label "Person" .

:Band a owl:Class ;
    rdfs:label "Band" .

:Role a owl:Class ;
    rdfs:label "Role" .

:BandMembership a owl:Class ;
    rdfs:label "Band Membership" ;
    rdfs:comment "A Band Membership represents the association of a person playing a role in a band during a specific time interval." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :involvesMember ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Person
    ] , [
        a owl:Restriction ;
        owl:onProperty :hasBand ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Band
    ] , [
        a owl:Restriction ;
        owl:onProperty :hasTimeInterval ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :TimeInterval
    ] , [
        a owl:Restriction ;
        owl:onProperty :hasRole ;
        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Role
    ] .

:TimeInterval a owl:Class ;
    rdfs:label "Time Interval" ;
    rdfs:comment "A Time Interval represents a period of time with a start date and an end date." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :startDate ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger
    ] , [
        a owl:Restriction ;
        owl:onProperty :endDate ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger
    ] .

######################
# Object Properties
######################

:hasMembership a owl:ObjectProperty ;
    rdfs:label "has membership" ;
    rdfs:comment "Relates a band to its band memberships." ;
    rdfs:domain :Band ;
    rdfs:range :BandMembership ;
    owl:inverseOf :isMembershipOf .

:isMembershipOf a owl:ObjectProperty ;
    rdfs:label "is membership of" ;
    rdfs:comment "Indicates the band of which this band membership is a part." ;
    rdfs:domain :BandMembership ;
    rdfs:range :Band .

:involvesMember a owl:ObjectProperty ;
    rdfs:label "involves member" ;
    rdfs:comment "Indicates the person involved in a band membership." ;
    rdfs:domain :BandMembership ;
    rdfs:range :Person ;
    owl:inverseOf :memberOfMembership .

:memberOfMembership a owl:ObjectProperty ;
    rdfs:label "member of membership" ;
    rdfs:comment "Indicates the band membership that this person is involved in." ;
    rdfs:domain :Person ;
    rdfs:range :BandMembership .

:hasRole a owl:ObjectProperty ;
    rdfs:label "has role" ;
    rdfs:comment "Indicates the role the person plays in the band during the membership." ;
    rdfs:domain :BandMembership ;
    rdfs:range :Role .

:hasTimeInterval a owl:ObjectProperty ;
    rdfs:label "has time interval" ;
    rdfs:comment "Specifies the time interval during which the band membership is valid." ;
    rdfs:domain :BandMembership ;
    rdfs:range :TimeInterval .

######################
# Data Properties
######################

:startDate a owl:DatatypeProperty ;
    rdfs:label "start date" ;
    rdfs:comment "The start date of a time interval." ;
    rdfs:domain :TimeInterval ;
    rdfs:range xsd:date .

:endDate a owl:DatatypeProperty ;
    rdfs:label "end date" ;
    rdfs:comment "The end date of a time interval." ;
    rdfs:domain :TimeInterval ;
    rdfs:range xsd:date .

