@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

######################
# Classes
######################

:Place a owl:Class ;
    rdfs:label "Place" ;
    rdfs:comment "A physical location or geographical area." .

:Country a owl:Class ;
    rdfs:subClassOf :Place ;
    rdfs:label "Country" ;
    rdfs:comment "A nation with its own government, occupying a particular territory." ;
    owl:disjointWith :Region .

:Region a owl:Class ;
    rdfs:subClassOf :Place ;
    rdfs:label "Region" ;
    rdfs:comment "An administrative area within a country." ;
    owl:disjointWith :Country ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :isPartOf ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Country
    ] .

######################
# Object Properties
######################

:isPartOf a owl:ObjectProperty , owl:TransitiveProperty ;
    rdfs:label "is part of" ;
    rdfs:comment "Indicates that one Place is part of another Place." ;
    rdfs:domain :Place ;
    rdfs:range :Place ;
    owl:inverseOf :hasPart .

:hasPart a owl:ObjectProperty , owl:TransitiveProperty ;
    rdfs:label "has part" ;
    rdfs:comment "Indicates that a Place has another Place as a part." ;
    rdfs:domain :Place ;
    rdfs:range :Place ;
    owl:inverseOf :isPartOf .

######################
# Data Properties
######################

:placeName a owl:DatatypeProperty ;
    rdfs:label "place name" ;
    rdfs:comment "The name of a Place." ;
    rdfs:domain :Place ;
    rdfs:range xsd:string .

