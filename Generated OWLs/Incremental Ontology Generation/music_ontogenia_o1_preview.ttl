######################
# Classes
######################

:Place a owl:Class ;
    rdfs:label "Place" ;
    rdfs:comment "A physical location or place." .

:Country a owl:Class ;
    rdfs:subClassOf :Place ;
    rdfs:label "Country" ;
    rdfs:comment "A nation with its own government." ;
    owl:disjointWith :Region, :City .

:Region a owl:Class ;
    rdfs:subClassOf :Place ;
    rdfs:label "Region" ;
    rdfs:comment "An administrative division or area within a country." ;
    owl:disjointWith :Country, :City ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :isPartOf ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Country
    ] .

:City a owl:Class ;
    rdfs:subClassOf :Place ;
    rdfs:label "City" ;
    rdfs:comment "An urban area, typically with a large population." ;
    owl:disjointWith :Country, :Region ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :isPartOf ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Region
    ] .

######################
# Object Properties
######################

:isPartOf a owl:ObjectProperty , owl:TransitiveProperty ;
    rdfs:label "is part of" ;
    rdfs:comment "Indicates that one Place is part of another Place." ;
    rdfs:domain :Place ;
    rdfs:range :Place ;
    owl:inverseOf :hasPart .

:hasPart a owl:ObjectProperty , owl:TransitiveProperty ;
    rdfs:label "has part" ;
    rdfs:comment "Indicates that a Place has another Place as a part." ;
    rdfs:domain :Place ;
    rdfs:range :Place ;
    owl:inverseOf :isPartOf .

######################
# Data Properties
######################

:placeName a owl:DatatypeProperty ;
    rdfs:label "place name" ;
    rdfs:comment "The name of a Place." ;
    rdfs:domain :Place ;
    rdfs:range xsd:string .

