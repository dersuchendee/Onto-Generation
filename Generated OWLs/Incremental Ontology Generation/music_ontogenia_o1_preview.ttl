@prefix : <http://example.org/music#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

######################
# Classes
######################

:Album a owl:Class ;
    rdfs:label "Album" .

:Recording a owl:Class ;
    rdfs:label "Recording" ;
    rdfs:comment "A Recording represents the process of recording an Album during a specific Time Interval by one or more Agents." ;
    rdfs:subClassOf :Event ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :recordedAlbum ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Album
    ] , [
        a owl:Restriction ;
        owl:onProperty :recordedDuring ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :TimeInterval
    ] , [
        a owl:Restriction ;
        owl:onProperty :recordedBy ;
        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Agent
    ] .

:Event a owl:Class ;
    rdfs:label "Event" ;
    rdfs:comment "An Event represents something that happens at a certain time." .

:TimeInterval a owl:Class ;
    rdfs:label "Time Interval" ;
    rdfs:comment "A period of time with a start date and an end date." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :startDate ;
