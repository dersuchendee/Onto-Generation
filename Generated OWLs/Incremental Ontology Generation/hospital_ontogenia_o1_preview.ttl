<http://example.org/ontology> rdf:type owl:Ontology .

### Classes

# Class: :Person

:Person rdf:type owl:Class ;
        rdfs:label "Person"@en ;
        rdfs:comment "A human being."@en .

# Class: :FieldOfStudy

:FieldOfStudy rdf:type owl:Class ;
              rdfs:label "Field of Study"@en ;
              rdfs:comment "An academic discipline or field of study."@en .

# Class: :MedicalField

:MedicalField rdf:type owl:Class ;
              rdfs:label "Medical Field"@en ;
              rdfs:comment "The field of medical sciences."@en ;
              rdfs:subClassOf :FieldOfStudy .

# Class: :Degree

:Degree rdf:type owl:Class ;
        rdfs:label "Degree"@en ;
        rdfs:comment "An academic degree or professional qualification."@en ;
        rdfs:subClassOf
          [ rdf:type owl:Restriction ;
            owl:onProperty :hasFieldOfStudy ;
            owl:someValuesFrom :FieldOfStudy
          ] .

# Class: :MedicalDegree

:MedicalDegree rdf:type owl:Class ;
               rdfs:label "Medical Degree"@en ;
               rdfs:comment "A degree related to the medical field."@en ;
               rdfs:subClassOf :Degree ,
                  [ rdf:type owl:Restriction ;
                    owl:onProperty :hasFieldOfStudy ;
                    owl:someValuesFrom :MedicalField
                  ] .

### Object Properties

# Object Property: :hasDegree

:hasDegree rdf:type owl:ObjectProperty ;
           rdfs:label "has degree"@en ;
           rdfs:comment "Relates a person to a degree they have obtained."@en ;
           rdfs:domain :Person ;
           rdfs:range :Degree .

# Object Property: :hasFieldOfStudy

:hasFieldOfStudy rdf:type owl:ObjectProperty ;
                 rdfs:label "has field of study"@en ;
                 rdfs:comment "Specifies the field of study of the degree."@en ;
                 rdfs:domain :Degree ;
                 rdfs:range :FieldOfStudy .

### Datatype Properties

# Datatype Property: :degreeName

:degreeName rdf:type owl:DatatypeProperty ;
            rdfs:label "degree name"@en ;
            rdfs:comment "The name or title of the degree."@en ;
            rdfs:domain :Degree ;
            rdfs:range xsd:string .

# Datatype Property: :degreeDate

:degreeDate rdf:type owl:DatatypeProperty ;
            rdfs:label "degree date"@en ;
            rdfs:comment "The date when the degree was obtained."@en ;
            rdfs:domain :Degree ;
            rdfs:range xsd:dateTime .

