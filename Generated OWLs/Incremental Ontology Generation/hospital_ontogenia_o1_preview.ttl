### Classes

# Class: :Person

:Person rdf:type owl:Class ;
        rdfs:label "Person"@en ;
        rdfs:comment "An individual human being."@en ;
        rdfs:subClassOf
          [ rdf:type owl:Restriction ;
            owl:onProperty :participatesIn ;
            owl:someValuesFrom :StudyEvent
          ] .

# Class: :Degree

:Degree rdf:type owl:Class ;
        rdfs:label "Degree"@en ;
        rdfs:comment "An academic degree or professional qualification."@en .

# Class: :StudyEvent

:StudyEvent rdf:type owl:Class ;
            rdfs:label "Study Event"@en ;
            rdfs:comment "An event where a person studies for a degree during a time interval."@en ;
            rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty :hasParticipant ;
                owl:someValuesFrom :Person
              ] ,
              [ rdf:type owl:Restriction ;
                owl:onProperty :hasDegree ;
                owl:someValuesFrom :Degree
              ] ,
              [ rdf:type owl:Restriction ;
                owl:onProperty :hasTimeInterval ;
                owl:someValuesFrom :TimeInterval
              ] .

# Class: :TimeInterval

:TimeInterval rdf:type owl:Class ;
              rdfs:label "Time Interval"@en ;
              rdfs:comment "A duration of time with a start and end date."@en .

### Object Properties

# Object Property: :participatesIn

:participatesIn rdf:type owl:ObjectProperty ;
                rdfs:label "participates in"@en ;
                rdfs:comment "Relates a person to an event they participate in."@en ;
                rdfs:domain :Person ;
                rdfs:range :StudyEvent ;
                owl:inverseOf :hasParticipant .

# Object Property: :hasParticipant

:hasParticipant rdf:type owl:ObjectProperty ;
                rdfs:label "has participant"@en ;
                rdfs:comment "Relates an event to its participant."@en ;
                rdfs:domain :StudyEvent ;
                rdfs:range :Person .

# Object Property: :hasDegree

:hasDegree rdf:type owl:ObjectProperty ;
           rdfs:label "has degree"@en ;
           rdfs:comment "Relates a study event to the degree being pursued."@en ;
           rdfs:domain :StudyEvent ;
           rdfs:range :Degree .

# Object Property: :hasTimeInterval

:hasTimeInterval rdf:type owl:ObjectProperty ;
                 rdfs:label "has time interval"@en ;
                 rdfs:comment "Relates an event to its time interval."@en ;
                 rdfs:domain :StudyEvent ;
                 rdfs:range :TimeInterval .

### Datatype Properties

# Datatype Property: :startDate

:startDate rdf:type owl:DatatypeProperty ;
           rdfs:label "start date"@en ;
           rdfs:comment "The starting date of a time interval."@en ;
           rdfs:domain :TimeInterval ;
           rdfs:range xsd:dateTime .

# Datatype Property: :endDate

:endDate rdf:type owl:DatatypeProperty ;
         rdfs:label "end date"@en ;
         rdfs:comment "The ending date of a time interval."@en ;
         rdfs:domain :TimeInterval ;
         rdfs:range xsd:dateTime .

