:Person rdf:type owl:Class ;
        rdfs:label "Person"@en ;
        rdfs:comment "An individual human being."@en .

# Class: :Organization

:Organization rdf:type owl:Class ;
              rdfs:label "Organization"@en ;
              rdfs:comment "An entity such as a company or institution that has a collective goal."@en .

# Class: :Hospital

:Hospital rdf:type owl:Class ;
          rdfs:label "Hospital"@en ;
          rdfs:comment "A health care institution providing patient treatment with specialized medical staff and equipment."@en ;
          rdfs:subClassOf :Organization .

# Class: :Employment

:Employment rdf:type owl:Class ;
            rdfs:label "Employment"@en ;
            rdfs:comment "An engagement of a person in work at an organization starting from a specific date."@en ;
            rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty :hasEmployee ;
                owl:someValuesFrom :Person
              ] ,
              [ rdf:type owl:Restriction ;
                owl:onProperty :hasEmployer ;
                owl:someValuesFrom :Organization
              ] ,
              [ rdf:type owl:Restriction ;
                owl:onProperty :employmentStartDate ;
                owl:someValuesFrom xsd:dateTime
              ] .

### Object Properties

# Object Property: :hasEmployee

:hasEmployee rdf:type owl:ObjectProperty ;
             rdfs:label "has employee"@en ;
             rdfs:comment "Relates an employment to the person who is employed."@en ;
             rdfs:domain :Employment ;
             rdfs:range :Person .

# Object Property: :hasEmployer

:hasEmployer rdf:type owl:ObjectProperty ;
             rdfs:label "has employer"@en ;
             rdfs:comment "Relates an employment to the organization that employs."@en ;
             rdfs:domain :Employment ;
             rdfs:range :Organization .

### Datatype Properties

# Datatype Property: :employmentStartDate

:employmentStartDate rdf:type owl:DatatypeProperty ;
                     rdfs:label "employment start date"@en ;
                     rdfs:comment "The date when the employment began."@en ;
                     rdfs:domain :Employment ;
                     rdfs:range xsd:dateTime .

