### Classes

# Class: :Person

:Person rdf:type owl:Class ;
        rdfs:label "Person"@en ;
        rdfs:comment "An individual human being."@en .

# Class: :Employee

:Employee rdf:type owl:Class ;
          rdfs:label "Employee"@en ;
          rdfs:comment "A person employed at an organization."@en ;
          rdfs:subClassOf :Person .

# Class: :Doctor

:Doctor rdf:type owl:Class ;
        rdfs:label "Doctor"@en ;
        rdfs:comment "A medical professional qualified to practice medicine."@en ;
        rdfs:subClassOf :Person .

# Class: :SeniorDoctor

:SeniorDoctor rdf:type owl:Class ;
              rdfs:label "Senior Doctor"@en ;
              rdfs:comment "A doctor with seniority or leadership responsibilities."@en ;
              rdfs:subClassOf :Doctor .

# Class: :EvaluationStatement

:EvaluationStatement rdf:type owl:Class ;
                     rdfs:label "Evaluation Statement"@en ;
                     rdfs:comment "A written statement evaluating an employee."@en ;
                     rdfs:subClassOf :Document ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :authoredBy ;
                          owl:allValuesFrom :Doctor ;
                          owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :aboutEmployee ;
                          owl:allValuesFrom :Employee ;
                          owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] .

# Class: :Document

:Document rdf:type owl:Class ;
          rdfs:label "Document"@en ;
          rdfs:comment "A record of information in any medium."@en .

### Object Properties

# Object Property: :authoredBy

:authoredBy rdf:type owl:ObjectProperty ;
            rdfs:label "authored by"@en ;
            rdfs:comment "Relates a document to the person who authored it."@en ;
            rdfs:domain :Document ;
            rdfs:range :Person .

# Object Property: :aboutEmployee

:aboutEmployee rdf:type owl:ObjectProperty ;
               rdfs:label "about employee"@en ;
               rdfs:comment "Relates a document to the employee it is about."@en ;
               rdfs:domain :Document ;
               rdfs:range :Employee .

# Object Property: :evaluates

:evaluates rdf:type owl:ObjectProperty ;
           rdfs:label "evaluates"@en ;
           rdfs:comment "A doctor evaluates an employee."@en ;
           rdfs:domain :Doctor ;
           rdfs:range :Employee ;
           owl:inverseOf :isEvaluatedBy .

# Object Property: :isEvaluatedBy

:isEvaluatedBy rdf:type owl:ObjectProperty ;
               rdfs:label "is evaluated by"@en ;
               rdfs:comment "An employee is evaluated by a doctor."@en ;
               rdfs:domain :Employee ;
               rdfs:range :Doctor ;
               owl:inverseOf :evaluates .

### Datatype Properties

# Datatype Property: :statementDate

:statementDate rdf:type owl:DatatypeProperty ;
               rdfs:label "statement date"@en ;
               rdfs:comment "The date when the evaluation statement was written."@en ;
               rdfs:domain :EvaluationStatement ;
               rdfs:range xsd:dateTime .

# Datatype Property: :statementText

:statementText rdf:type owl:DatatypeProperty ;
               rdfs:label "statement text"@en ;
               rdfs:comment "The textual content of the evaluation statement."@en ;
               rdfs:domain :EvaluationStatement ;
               rdfs:range xsd:string .

