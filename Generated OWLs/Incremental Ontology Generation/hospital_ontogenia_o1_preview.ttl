### Classes

# Class: :Person

:Person rdf:type owl:Class ;
        rdfs:label "Person"@en ;
        rdfs:comment "An individual human being."@en .

# Class: :Organization

:Organization rdf:type owl:Class ;
              rdfs:label "Organization"@en ;
              rdfs:comment "An entity such as a company or institution with a collective purpose."@en .

# Class: :Union

:Union rdf:type owl:Class ;
        rdfs:label "Union"@en ;
        rdfs:comment "An organization representing a group of workers."@en ;
        rdfs:subClassOf :Organization .

# Class: :TimeInterval

:TimeInterval rdf:type owl:Class ;
              rdfs:label "Time Interval"@en ;
              rdfs:comment "A duration of time with a start and end date."@en .

# Class: :UnionMembership

:UnionMembership rdf:type owl:Class ;
                 rdfs:label "Union Membership"@en ;
                 rdfs:comment "An engagement of a person as a member of a union during a time interval."@en ;
                 rdfs:subClassOf
                   [ rdf:type owl:Restriction ;
                     owl:onProperty :hasMember ;
                     owl:someValuesFrom :Person
                   ] ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty :hasUnion ;
                     owl:someValuesFrom :Union
                   ] ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty :hasTimeInterval ;
                     owl:someValuesFrom :TimeInterval
                   ] .

