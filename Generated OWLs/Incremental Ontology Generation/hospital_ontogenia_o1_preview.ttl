### Classes

# Class: :Agent

:Agent rdf:type owl:Class ;
    rdfs:label "Agent"@en ;
    rdfs:comment "An agentive object, either physical or social."@en .

# Class: :Person

:Person rdf:type owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:comment "An individual human being."@en ;
    rdfs:subClassOf :Agent .

# Class: :Role

:Role rdf:type owl:Class ;
    rdfs:label "Role"@en ;
    rdfs:comment "A role that an agent can play."@en .

# Class: :UnionRole

:UnionRole rdf:type owl:Class ;
    rdfs:label "Union Role"@en ;
    rdfs:comment "A role within a union group."@en ;
    rdfs:subClassOf :Role .

# Class: :Union

:Union rdf:type owl:Class ;
    rdfs:label "Union"@en ;
    rdfs:comment "An organization representing a group of workers."@en .

# Class: :TimeInterval

:TimeInterval rdf:type owl:Class ;
    rdfs:label "Time Interval"@en ;
    rdfs:comment "A period of time with a start and end date."@en .

# Class: :UnionRoleAssignment

:UnionRoleAssignment rdf:type owl:Class ;
    rdfs:label "Union Role Assignment"@en ;
    rdfs:comment "An assignment of a union role to an agent within a union, at a certain time."@en ;
    rdfs:subClassOf
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasAgent ;
          owl:someValuesFrom :Agent
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasRole ;
          owl:someValuesFrom :UnionRole
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :inUnion ;
          owl:someValuesFrom :Union
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :atTime ;
          owl:someValuesFrom :TimeInterval
        ] .

### Object Properties

# Object Property: :hasAgent

:hasAgent rdf:type owl:ObjectProperty ;
    rdfs:label "has agent"@en ;
    rdfs:comment "Relates a union role assignment to the agent playing the role."@en ;
    rdfs:domain :UnionRoleAssignment ;
    rdfs:range :Agent .

# Object Property: :hasRole

:hasRole rdf:type owl:ObjectProperty ;
    rdfs:label "has role"@en ;
    rdfs:comment "Relates a union role assignment to the role assigned."@en ;
    rdfs:domain :UnionRoleAssignment ;
    rdfs:range :UnionRole .

# Object Property: :inUnion

:inUnion rdf:type owl:ObjectProperty ;
    rdfs:label "in union"@en ;
    rdfs:comment "Relates a union role assignment to the union."@en ;
    rdfs:domain :UnionRoleAssignment ;
    rdfs:range :Union .

# Object Property: :atTime

:atTime rdf:type owl:ObjectProperty ;
    rdfs:label "at time"@en ;
    rdfs:comment "Relates a union role assignment to the time interval."@en ;
    rdfs:domain :UnionRoleAssignment ;
    rdfs:range :TimeInterval .

### Datatype Properties

# Datatype Property: :startDate

:startDate rdf:type owl:DatatypeProperty ;
    rdfs:label "start date"@en ;
    rdfs:comment "The start date of a time interval."@en ;
    rdfs:domain :TimeInterval ;
    rdfs:range xsd:dateTime .

# Datatype Property: :endDate

:endDate rdf:type owl:DatatypeProperty ;
    rdfs:label "end date"@en ;
    rdfs:comment "The end date of a time interval."@en ;
    rdfs:domain :TimeInterval ;
    rdfs:range xsd:dateTime .

