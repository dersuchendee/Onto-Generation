:hasMember rdf:type owl:ObjectProperty ;
           rdfs:label "has member"@en ;
           rdfs:comment "Relates a union membership to the person who is a member."@en ;
           rdfs:domain :UnionMembership ;
           rdfs:range :Person ;
           owl:inverseOf :participatesIn .

# Object Property: :hasUnion

:hasUnion rdf:type owl:ObjectProperty ;
          rdfs:label "has union"@en ;
          rdfs:comment "Relates a union membership to the union involved."@en ;
          rdfs:domain :UnionMembership ;
          rdfs:range :Union ;
          owl:inverseOf :hasMembership .

# Object Property: :hasTimeInterval

:hasTimeInterval rdf:type owl:ObjectProperty ;
                 rdfs:label "has time interval"@en ;
                 rdfs:comment "Relates an event to its time interval."@en ;
                 rdfs:domain :UnionMembership ;
                 rdfs:range :TimeInterval .

# Object Property: :participatesIn

:participatesIn rdf:type owl:ObjectProperty ;
                rdfs:label "participates in"@en ;
                rdfs:comment "Relates a person to a union membership they participate in."@en ;
                rdfs:domain :Person ;
                rdfs:range :UnionMembership .

# Object Property: :hasMembership

:hasMembership rdf:type owl:ObjectProperty ;
               rdfs:label "has membership"@en ;
               rdfs:comment "Relates a union to its memberships."@en ;
               rdfs:domain :Union ;
               rdfs:range :UnionMembership .

